#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/rollup@3.25.1/node_modules/rollup/dist/bin/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/rollup@3.25.1/node_modules/rollup/dist/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/rollup@3.25.1/node_modules/rollup/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/rollup@3.25.1/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/rollup@3.25.1/node_modules/rollup/dist/bin/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/rollup@3.25.1/node_modules/rollup/dist/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/rollup@3.25.1/node_modules/rollup/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/rollup@3.25.1/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
