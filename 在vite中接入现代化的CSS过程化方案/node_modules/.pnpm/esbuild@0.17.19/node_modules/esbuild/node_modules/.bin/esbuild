#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/esbuild@0.17.19/node_modules/esbuild/bin/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/esbuild@0.17.19/node_modules/esbuild/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/esbuild@0.17.19/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/esbuild@0.17.19/node_modules/esbuild/bin/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/esbuild@0.17.19/node_modules/esbuild/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/esbuild@0.17.19/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esbuild" "$@"
else
  exec node  "$basedir/../../bin/esbuild" "$@"
fi
