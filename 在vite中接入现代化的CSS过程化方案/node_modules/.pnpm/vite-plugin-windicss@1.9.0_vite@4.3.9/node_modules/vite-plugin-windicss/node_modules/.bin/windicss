#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/windicss@3.5.6/node_modules/windicss/cli/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/windicss@3.5.6/node_modules/windicss/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/windicss@3.5.6/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/windicss@3.5.6/node_modules/windicss/cli/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/windicss@3.5.6/node_modules/windicss/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/windicss@3.5.6/node_modules:/mnt/d/webpack-vite/vite/vite-study/在vite中接入现代化的CSS过程化方案/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../windicss@3.5.6/node_modules/windicss/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../windicss@3.5.6/node_modules/windicss/cli/index.js" "$@"
fi
